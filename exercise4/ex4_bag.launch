<launch>
<!-- RVIZ -->
<node pkg="rviz" type="rviz" name="rviz">
</node>

<!-- GAZEBO -->
<include file="$(find husky_gazebo)/launch/husky_empty_world.launch">
    <arg name="world_name" value="$(find husky_highlevel_controller)/worlds/singlePillar.world"/>
    <arg name="laser_enabled" value="true"/>
    <rosparam command="load"
        file="$(find husky_control)/config/localization.yaml"/>
</include>

<!-- ROSBAG -->
<node pkg="rosbag" type="play" name="rosbag" arg="--clock ~/git/robotprogramming/exercise4/husky_navigation.bag" />

</launch>
